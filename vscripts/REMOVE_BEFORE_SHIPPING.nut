array<string> function GenerateSquirrelCodeArray( string str, int indent = 15 )
{
	array<string> sampled = split( str, " " )
	array<string> generated
	
	generated.append( "array<string> items = \n[ \n" )
	string spacing = TableIndent( indent )
	
	foreach( txt in sampled )
		generated.append( spacing + "\"" + txt + "\"\n" )
		
	generated.append( "];" )
	
	#if DEVELOPER	
		string generatedCodeString = ""
		
		foreach( gened in generated )
			generatedCodeString += gened 
			
		printl( "\n\n" + generatedCodeString )
	#endif
		
	return generated
}

