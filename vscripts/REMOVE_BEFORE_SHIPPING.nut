string function RemoveAllFromString_IfInArray( string str, array<string> invalid )
{
	string parse = str
	
	if( empty( parse ) )
		return parse
	
	if( invalid.len() == 0 )
		return parse
	
	foreach( char in invalid )
		parse = StringReplace( parse, char, ""  )
		
	return parse
}

