	#endif
	
	entity clientPlayer = GetLocalClientPlayer()
	if ( IsValid ( clientPlayer ) )
		Signal( clientPlayer, "DeathScreenMenuClosed" )

	SetCurrentTabIndex( eDeathScreenPanel.NONE )
