bool function ClientCommand_FriendlyFireFR(entity player, array<string> args)
//By michae\l/#1125 & Retículo Endoplasmático#5955.
{
printt("Flowstate DEBUG - A player has toggled friendly fire.", player)
array<entity> connectedPlayers = GetPlayerArray_ConnectedNotSpectatorTeam()
int playersON = connectedPlayers.len()
array<entity> randomizedPlayers = shuffleArray(GetPlayerArray())
	
float cooldown = 5
if( Time() - lasttimeToggleFriendlyFire < cooldown )
    {
		return false
	}
	
if(!toggleFr) {
		int currentTeam = 2 //2
		int numTeams = playersON


		foreach (p in randomizedPlayers)
		{
			if (IsValid(p)) {SetTeam(p,TEAM_IMC + 1 + (currentTeam % playersON))}
							currentTeam += 1
		}
		foreach (sPlayer in GetPlayerArray()){
				Message(sPlayer, "FRIENDLY FIRE: ON", "Changed by: " + player.GetPlayerName(), 0.5)
				}
				lasttimeToggleFriendlyFire = Time()
				toggleFr = true
			return true 
	}else {
		
			foreach (p in randomizedPlayers)
			{
				if (IsValid(p)) SetTeam(p,TEAM_IMC)
			}
		
				foreach (sPlayer in GetPlayerArray()){
				Message(sPlayer, "FRIENDLY FIRE: OFF", "Changed by: " + player.GetPlayerName(), 0.5)
				}
		lasttimeToggleFriendlyFire = Time()
		toggleFr = false
		return true 
	}
		return true
}


