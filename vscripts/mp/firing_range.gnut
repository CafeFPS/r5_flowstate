void function FiringRangeGameMode_Init()
{

	PrecacheModel( FIRINGRANGE_BLUE_STAND_ASSET )
	PrecacheModel( FIRINGRANGE_FLICK_TARGET_ASSET )
	PrecacheParticleSystem( FIRINGRANGE_FLICK_TARGET_PARTICLE_HIT )
	PrecacheParticleSystem( FIRINGRANGE_ITEM_RESPAWN_PARTICLE )
	
	#if SERVER
		if(GetMapName() != "mp_rr_canyonlands_staging" || GameRules_GetGameMode() == "custom_tdm") return
		
		AddCallback_EntitiesDidLoad( OnEntitiesDidLoadFR )
		AddCallback_OnClientConnected( _WaitForPlayerSpawnFR )
		AddCallback_OnPlayerKilled( _OnPlayerKilledFR )
		AddCallback_OnWeaponAttack( OnWeaponAttack )
		AddSpawnCallback( "prop_survival", OnPropSurvivalSpawned )
		AddClientCommandCallback("frfire", ClientCommand_FriendlyFireFR)
		SetGameState(eGameState.Playing)
		WaitForGameState(eGameState.Playing)
		thread ThreadThatWatchesForWeaponsCuzWhenPlayerIsDedDoesntHaveWeaponsWtf()
	#endif
}



