void function ThreadThatWatchesForWeaponsCuzWhenPlayerIsDedDoesntHaveWeaponsWtf()
{
	while(true)
	{
		foreach(player in GetPlayerArray())
		{
			if(IsValid(player) && IsAlive(player)){
			player.p.storedWeapons = StoreWeapons(player)

			}
		}
		wait 1
	}
}

void function _HandleRespawnFR(entity player, bool respawnsameposition = false)
{
	//BY COLOMBIA
	if(IsValid(player) && !IsAlive(player)){
			wait 1
			
		if(respawnsameposition){
				vector lastPos = player.GetOrigin()
				vector lastAng = player.GetAngles()
				//player.p.storedWeapons = StoreWeapons(player)
				array<ItemFlavor> characters = clone GetAllCharacters()
				ItemFlavor PersonajeEscogido = characters[RandomInt(10)]
				CharacterSelect_AssignCharacter( ToEHI( player ), PersonajeEscogido )
				player.SetOrigin(lastPos)
				player.SetAngles(lastAng)
				DecideRespawnPlayer( player )
				
				TakeAllWeapons( player )
				GiveWeaponsFromStoredArray( player, player.p.storedWeapons )
							entity primary = player.GetNormalWeapon( WEAPON_INVENTORY_SLOT_PRIMARY_0 )
			entity secondary = player.GetNormalWeapon( WEAPON_INVENTORY_SLOT_PRIMARY_1 )
			FixWeapon(primary)
			FixWeapon(secondary)
			player.p.storedWeapons.clear()
		
			} else {
				array<ItemFlavor> characters = clone GetAllCharacters()
				ItemFlavor PersonajeEscogido = characters[RandomInt(10)]
				CharacterSelect_AssignCharacter( ToEHI( player ), PersonajeEscogido )
				DecideRespawnPlayer( player )
				SetRandomStagingPositionForPlayer( player )
				TakeAllWeapons(player)
				entity primary = player.GiveWeapon("mp_weapon_wingman", WEAPON_INVENTORY_SLOT_PRIMARY_0, ["highcal_mag_l3", "optic_cq_hcog_classic", "highcal_mag_l3"])
				entity secondary = player.GiveWeapon("mp_weapon_r97", WEAPON_INVENTORY_SLOT_PRIMARY_1, ["bullets_mag_l3","optic_cq_hcog_classic","barrel_stabilizer_l4_flash_hider","stock_tactical_l3"])
				FixWeapon(primary)
				FixWeapon(secondary)
				player.SetActiveWeaponByName( eActiveInventorySlot.mainHand, "mp_weapon_wingman" )
				player.GiveWeapon( "mp_weapon_melee_survival", WEAPON_INVENTORY_SLOT_PRIMARY_2, [] )
				player.GiveOffhandWeapon( "melee_data_knife", OFFHAND_MELEE, [] )
		}
		player.kv.solid = 6
		GivePassive(player, ePassives.PAS_PILOT_BLOOD)
		Inventory_SetPlayerEquipment(player, "armor_pickup_lv1", "armor")
		player.SetShieldHealth( 50 )
		player.TakeOffhandWeapon( OFFHAND_TACTICAL )
		player.TakeOffhandWeapon( OFFHAND_ULTIMATE )
		GiveRandomTac(player)
		GiveRandomUlt(player)
		Remote_CallFunction_Replay( player, "ServerCallback_RefreshInventoryAndWeaponInfo" )
	}
}

