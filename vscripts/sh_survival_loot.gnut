	catToTitle[ eLootSortCategories.AMMO ] <- "#LOOT_CAT_AMMO"
	catToTitle[ eLootSortCategories.WEAPONS ] <- "#LOOT_CAT_MAINWEAPON"
#if(false)

#endif
	catToTitle[ eLootSortCategories.DEFAULT ] <- ""

	return catToTitle[ cat ]
}

