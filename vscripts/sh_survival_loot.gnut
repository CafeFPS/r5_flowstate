	if ( ent.GetTeam() != TEAM_UNASSIGNED )
	{
		if ( ent.GetTeam() == player.GetTeam() && IsValid( ent.GetOwner() ) )
			return true
	}

	return false
}

void function OnDeathBoxUse( entity box, entity player, int useInputFlags )
{
	if ( useInputFlags & USE_INPUT_LONG )
