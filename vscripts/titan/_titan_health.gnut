float function GetShieldRegenTime( entity soul )
{
	float time
	if ( SoulHasPassive( soul, ePassives.PAS_SHIELD_REGEN ) )
		time = TITAN_SHIELD_REGEN_TIME * 0.5
	else
		time = TITAN_SHIELD_REGEN_TIME

	return time
}

